<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tool="http://schemas.android.com/tools"
    tool:parentTag="androidx.constraintlayout.widget.ConstraintLayout">

    <View
        android:id="@+id/line_divider_left"
        android:layout_width="40dp"
        android:layout_height="1dp"
        android:background="@color/topDividerLine"
        app:layout_constraintBottom_toBottomOf="@id/tv_center_text"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/tv_center_text"
        app:layout_constraintTop_toTopOf="@id/tv_center_text" />

    <TextView
        android:id="@+id/tv_center_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:gravity="center"
        android:text="@string/not_complete_num"
        android:textColor="@color/topDividerText"
        android:textSize="13sp"
        app:layout_constraintLeft_toRightOf="@id/line_divider_left"
        app:layout_constraintRight_toLeftOf="@id/line_divider_right"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/line_divider_right"
        android:layout_width="40dp"
        android:layout_height="1dp"
        android:background="@color/topDividerLine"
        app:layout_constraintBottom_toBottomOf="@id/tv_center_text"
        app:layout_constraintLeft_toRightOf="@id/tv_center_text"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@id/tv_center_text" />
</merge>